{
  "name": "community-oasis-dashboard",
  "private": true,
  "version": "0.1.0",
  "description": "A webapp for a community management platform, designed to fit the needs of retirement communities.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel serve src/index.html",
    "build": "npm run build:functions && npm run build:webapp",
    "build:webapp": "parcel build src/index.html",
    "build:functions": "npm --prefix functions/ run build",
    "serve": "firebase emulators:start",
    "serve:functions": "npm --prefix functions/ run serve",
    "shell": "npm --prefix functions run shell",
    "deploy:functions": "npm --prefix functions run deploy",
    "start": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ADolbyB/community-oasis-dashboard.git"
  },
  "keywords": [
    "react",
    "react-app",
    "parcel",
    "web-app",
    "progressive-web-app"
  ],
  "authors": [
    "Joel Brigida",
    "Hakeem Johnson",
    "Joshua Lavieri",
    "Michael Mesquita",
    "Sam 'Tell-Me-Your-Last-Name'"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ADolbyB/community-oasis-dashboard/issues"
  },
  "homepage": "https://github.com/ADolbyB/community-oasis-dashboard#readme",
  "dependencies": {
    "firebase-tools": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "parcel": "^2.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
